{
  "name": "fast-classifieds",
  "productName": "Fast Classifieds",
  "version": "1.6.0",
  "description": "An Electron app to quickly browse classified ads sites.",
  "main": ".vite/build/main.js",
  "scripts": {
    "dev": "electron-forge start",
    "package": "SHOULD_APPLE_SIGN=0 electron-forge package && open ./out",
    "make": "npm run check-version && SHOULD_APPLE_SIGN=1 dotenv -e .env -- electron-forge make && open ./out",
    "publish": "npm run check-version && SHOULD_APPLE_SIGN=1 dotenv -e .env -- electron-forge publish && open ./out",
    "lint": "biome check",
    "lint:fix": "biome format",
    "db:generate": "drizzle-kit generate --config ./drizzle.config.ts",
    "db:apply": "drizzle-kit push --config ./drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./drizzle.config.ts",
    "typecheck": "tsc -p tsconfig.main.json --noEmit && tsc -p tsconfig.renderer.json --noEmit",
    "check-version": "node --no-warnings ./scripts/check-version.js",
    "prepare": "husky",
    "bootstrap": "./scripts/bootstrap.sh"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/TravisBumgarner/fast-classifieds.git"
  },
  "author": {
    "name": "Travis Bumgarner",
    "email": "11890057+TravisBumgarner@users.noreply.github.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-vite": "^7.10.2",
    "@electron-forge/publisher-github": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "latest",
    "drizzle-orm": "latest",
    "electron": "^38.2.2",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.9"
  },
  "dependencies": {
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@libsql/client": "^0.15.15",
    "@mui/material": "^7.3.4",
    "@preact/signals-react": "^3.3.1",
    "@sentry/electron": "^7.2.0",
    "@tanstack/react-query": "^5.90.5",
    "better-sqlite3": "^12.4.1",
    "electron-log": "^5.4.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^11.0.2",
    "esbuild-register": "^3.6.0",
    "jsdom": "^27.1.0",
    "openai": "^6.8.1",
    "puppeteer": "^24.29.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.4",
    "update-electron-app": "^3.1.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  }
}
